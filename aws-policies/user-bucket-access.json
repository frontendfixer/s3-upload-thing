{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "OriginalBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::file-bucket/*"
        },
        {
            "Sid": "ThumbnailBucketAccess",
            "Effect": "Allow",
            "Action": [
                "s3:PutObject",
                "s3:GetObject",
                "s3:DeleteObject"
            ],
            "Resource": "arn:aws:s3:::thumbnail-bucket/*"
        }
    ]
}
